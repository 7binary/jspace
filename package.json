{
  "name": "jspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"yarn start:server\" \"yarn start:client\"",
    "build": "concurrently --kill-others \"yarn build:server\" \"yarn build:client\"",
    "start:client": "yarn --cwd \"client\" start",
    "start:server": "yarn --cwd \"server\" start",
    "build:client": "yarn --cwd \"client\" build",
    "build:server": "yarn --cwd \"server\" build"
  },
